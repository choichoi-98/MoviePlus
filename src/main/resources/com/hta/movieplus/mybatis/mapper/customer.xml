<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hta.movieplus.mybatis.mapper.CustomerMapper">
	
	<!-- mapperjava의 메서드값과 동일하게 id값 통일    파라미터 타입은 vo의 이름과 동일하게 앞자미나 소문자 -->
	<insert id="insertCumtomerOneOnOneVO" parameterType="customerOneOnOneVO">
		INSERT INTO CUSTOMER_ONEONONE
		VALUES(#{VO의 이름과 통일}, #{})
	</insert>	
	
</mapper>